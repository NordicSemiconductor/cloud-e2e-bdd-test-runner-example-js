{
  "name": "@nordicsemiconductor/e2e-bdd-test-runner-example",
  "version": "0.0.0-development",
  "description": "Example use of @nordicsemiconductor/bdd-markdown",
  "type": "module",
  "scripts": {
    "test": "npx tsx features/run-features.ts | npx tsx features/console-reporter.ts",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/cloud-e2e-bdd-test-runner-example-js.git"
  },
  "keywords": [
    "bdd",
    "aws",
    "cloudnative",
    "e2e",
    "bdd",
    "testing"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/cloud-e2e-bdd-test-runner-example-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/cloud-e2e-bdd-test-runner-example-js#readme",
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "3.171.0",
    "@aws-sdk/client-sqs": "3.171.0",
    "@aws-sdk/client-xray": "3.171.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.126",
    "@nordicsemiconductor/bdd-markdown": "3.2.1",
    "@nordicsemiconductor/cloudformation-helpers": "7.0.6",
    "@types/aws-lambda": "^8.10.104",
    "@types/glob": "8.0.0",
    "@types/node": "18.7.18",
    "@types/yazl": "2.4.2",
    "aws-cdk-lib": "2.42.1",
    "cdk": "2.42.1",
    "constructs": "10.1.108",
    "dependency-tree": "8.1.2",
    "eslint-plugin-import": "2.26.0",
    "glob": "8.0.3",
    "node-fetch": "3.2.10",
    "tsx": "3.9.0",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "dependencies": {
    "@aws-lambda-powertools/tracer": "1.2.1"
  }
}
