{
  "name": "@nordicsemiconductor/e2e-bdd-test-runner-example",
  "version": "0.0.0-development",
  "description": "Example use of @nordicsemiconductor/e2e-bdd-test-runner",
  "main": "index.js",
  "scripts": {
    "test": "node dist/feature-runner/run-features.js",
    "prettier": "prettier --write \"./**/*.{ts,js,json}\"",
    "prettier:lint": "prettier --list-different \"./**/*.{ts,js,json}\"",
    "lint": "npm run tslint && npm run prettier:lint",
    "tslint": "eslint --ext .js,.ts \"./{feature-runner,aws}/*.ts\"",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/cloud-e2e-bdd-test-runner-example-js.git"
  },
  "keywords": [
    "bdd",
    "aws",
    "cloud",
    "cloudnative",
    "gherkin",
    "testing"
  ],
  "author": "Markus Tacker <m@coderbyheart.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/cloud-e2e-bdd-test-runner-example-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/cloud-e2e-bdd-test-runner-example-js#readme",
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "1.89.0",
    "@aws-cdk/aws-codebuild": "1.89.0",
    "@aws-cdk/aws-codepipeline": "1.89.0",
    "@aws-cdk/aws-iam": "1.89.0",
    "@aws-cdk/aws-lambda": "1.89.0",
    "@aws-cdk/aws-logs": "1.89.0",
    "@aws-cdk/aws-s3": "1.89.0",
    "@aws-cdk/aws-sqs": "1.89.0",
    "@aws-cdk/aws-ssm": "1.89.0",
    "@aws-cdk/core": "1.89.0",
    "@aws-sdk/client-cloudformation": "3.4.1",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-angular": "11.0.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "9.0.2",
    "@nordicsemiconductor/cloudformation-helpers": "6.0.4",
    "@nordicsemiconductor/e2e-bdd-test-runner": "12.0.5",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "7.0.2",
    "@types/node": "14.14.26",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "aws-cdk": "1.89.0",
    "eslint": "7.19.0",
    "husky": "5.0.9",
    "lint-staged": "10.5.4",
    "pinst": "2.1.4",
    "prettier": "2.2.1",
    "typescript": "4.1.5"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "release": {
    "branch": "saga",
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  }
}
